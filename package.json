{
  "name": "puppeteer-testing",
  "version": "1.0.0",
  "description": "Puppeteer Testing",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter mochawesome --reporter-options reportDir=report,reportFilename=MochaReport --timeout=10000 ./tests",
    "test:snapshot": "jest --config=jest.config.js --detectOpenHandles --forceExit",
    "test:snapshot:update": "jest --config=jest.config.js --updateSnapshot --detectOpenHandles --forceExit"
  },
  "author": "Tylek Kadyrov",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.2.0",
    "jest": "^24.8.0",
    "jest-image-snapshot": "^2.8.1",
    "mocha": "^6.2.0",
    "mochawesome": "^4.0.1",
    "puppeteer": "^1.19.0"
  }
}
